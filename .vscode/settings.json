{
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.fixAll": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "files.exclude": {
    ".vscode": true,
    "node_modules": true
  },
  "search.exclude": {
    "**/*.gen.*": true,
    "node_modules/**": true,
    "*.lock": true
  },
  "files.readonlyInclude": {
    "**/*.gen.*": true,
    "node_modules/**": true,
    "*.lock": true
  }
}
